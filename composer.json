{
    "name": "manojkiran/bladecomponents",
    "description": "Some of the useful Blade Components",
    "keywords": [
        "manojkiran",
        "bladecomponents"
    ],
    "homepage": "https://github.com/manojkiran/bladecomponents",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Manojkiran Appathurai",
            "email": "manojkiran1003998@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "illuminate/support": "^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^9.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Manojkiran\\BladeComponents\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Manojkiran\\Bladecomponents\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Manojkiran\\Bladecomponents\\BladecomponentsServiceProvider"
            ],
            "aliases": {
                "Bladecomponents": "Manojkiran\\Bladecomponents\\BladecomponentsFacade"
            }
        }
    }
}
